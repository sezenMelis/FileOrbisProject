<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FileOrbisProject.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FileOrbisProject"
    Shell.FlyoutBehavior="Disabled"
    Title="FileOrbisProject">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <FlyoutItem Route="HomePage" FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent Title="Client" Icon="{x:Static models:Icons.AboutUs}" ContentTemplate="{DataTemplate views:ClientPage}"/>
    </FlyoutItem>
</Shell>
